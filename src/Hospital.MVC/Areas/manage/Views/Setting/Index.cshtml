@model List<Setting>
@{
    var counter = 0;
}

<div class="container">


    <table class="table">
        <thead>
            <tr>
                <th scope="col">No</th>
                <th scope="col">Value</th>
                <th scope="col">Key</th>

                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var setting in Model)
            {
                counter++;
                <tr>

                    <th scope="row">@counter</th>
                    <td>@setting.Value</td>
                    <td>@setting.Key</td>

                    <td>
                        <a asp-action="update" asp-controller="setting" asp-route-id="@setting.Id" class="btn btn-success">Update</a>

                    </td>

                </tr>
            }


        </tbody>
    </table>
</div>
